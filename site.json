{
  "baseUrl": "/test-md",
  "titlePrefix": "1",
  "style": {
    "codeTheme": "dark"
  },
  "ignore": [
    "_markbind/layouts/*",
    "_markbind/logs/*",
    "_site/*",
    "site.json",
    "*.md",
    "*.mbd",
    "*.mbdf",
    "*.njk",
    ".git/*"
  ],
  "pages": [
    {
      "src": "index.md",
      "title": "Landing Page"
    },
    {
      "glob": ["**/index.md", "**/*.+(md|mbd)"]
    }
  ],
  "plugins": ["codeBlockCopyButtons"],
  "deploy": {
    "message": "Site Update."
  },
  "locale": "af-NA",
  "intrasiteLinkValidation": {
    "enabled": true
  }
}
